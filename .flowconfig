[ignore]
.*/node_modules/.*
.*/dist/.*

[include]
.*/src/.*

[libs]
flow-typed

[lints]
all=warn
untyped-type-import=error
sketchy-null-bool=off

[options]
module.system.node.resolve_dirname=src
emoji=true
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
